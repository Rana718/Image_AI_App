generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model UserList{
  id Int @id @default(autoincrement())
  userName String
  userEmail String @unique
  credits Int
}


model aiModelList{
  id Int @id @default(autoincrement())
  name String
  aiModelName String
  defaultPrompt String?
  isFeatured Boolean
  userImageUpload Boolean
  style Boolean
  avater Boolean
  icon String?
  banner String?
}