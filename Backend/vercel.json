{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "prisma/**",
                    "node_modules/.prisma/**",
                    "package.json"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        }
    ],
    "installCommand": "bun install",
    "buildCommand": "bun run build",
    "outputDirectory": "dist"
}